<p id="notice"><%= notice %></p>

<h1>Api V1 Contributions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_contributions.each do |api_v1_contribution| %>
      <tr>
        <td><%= link_to 'Show', api_v1_contribution %></td>
        <td><%= link_to 'Edit', edit_api_v1_contribution_path(api_v1_contribution) %></td>
        <td><%= link_to 'Destroy', api_v1_contribution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Contribution', new_api_v1_contribution_path %>
